import java.util.*;
class PowerX{
    public static void main(String[] args) throws Exception {
        Scanner in = new Scanner(System.in);
        int N;
        N = in.nextInt();
        int X;
        X = in.nextInt();
        System.out.print(powerx(N,X));
    }
    static int powerx(int N,int X){
        if(X==0){
            return 1;
        }
        return N*powerx(N,X-1);
    }
 
}
