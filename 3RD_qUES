import java.util.*;
class TwoSum {
    public static void main(String[] args) throws Exception {
         int[] arr = {1,2,3,4,5}; 
         int target = 7; 
         for(int i=0;i<arr.length;i++){
             for(int j=i+1;j<arr.length;j++){ 
                 if((target-arr[i])==arr[j]){ 
                     System.out.println("["+i+","+j+"]");
                     System.out.println( );
                } 
            }
        }
        System.out.println("-1");
    }
